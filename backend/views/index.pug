html
    head
        title= title
        link(rel="stylesheet", href="//cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.2.12/semantic.min.css")
     


    body
        div(class='ui grid container')
            div
                br
                h1= message1
                p= submessage1
                h5 Last imported course:
                div(class='ui divided items')
                    div(class='item')
                        if(values2.length)
                            form(method="POST")
                                div(class='left floated content')
                                    input(type="submit", value="Import", class="ui disabled blue button")
                                div(class='middle aligned content')
                                    h3(class='ui disabled header')= values2[0].name + " - " + values2[0].ohid    

                    each val in values1
                        div(class='item')   
                            form(method="POST")
                                input(name="hid" type="hidden" value=val.id)
                                input(name="cname" type="hidden" value=val.name)
                                input(name="starts" type="hidden" value=val.starts)
                                input(name="ends" type="hidden" value=val.ends)
                                div(class='left floated content')
                                    input(type="submit", value="Import", class="ui blue button")
                                div(class='middle aligned content')
                                    h3(class='ui header')= val.name + " - " + val.id                 
                    else
                        br
                        p= elseMessage1

            div
                br
                h1= message2
                div(class='ui divided items')
                    each val in values2
                        div(class='item')   
                            form(method="POST")
                                input(name="hid" type="hidden" value=val.id)
                                input(name="cname" type="hidden" value=val.name)
                                input(name="starts" type="hidden" value=val.starts)
                                input(name="ends" type="hidden" value=val.ends)
                                div(class='left floated content')
                                    input(type="submit", value="Import", class="ui disabled blue button")
                                div(class='middle aligned content')
                                    h3(class='ui disabled header')= val.name + " - " + val.ohid                
                    else
                        br
                        p= elseMessage2

        